<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <h2>Edit page</h2>

      <%= link_to("<< Back to List", pages_path, class: "back-link") %>

      <div class="pages new">
        <%= form_for(@page) do |f| %>

            <%= render(:partial => "form", :locals => {:f => f}) %>

            <%= f.submit("Update Page", class: "btn btn-primary") %>
        <% end %>
      </div>

    </div>
  </div>
</div>